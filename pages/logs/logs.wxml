<!--logs.wxml-->
<view hidden='true' class="container log-list">
  <block wx:for="{{logs}}" wx:for-item="log">
    <text class="log-item">{{index + 1}}. {{log}}</text>
  </block>
</view>

<!--画板界面-->

<canvas class="draw_label" style='background-color:#ECECEC;' hidden='true' canvas-id="firstCanvas" disable-scroll="true" bindtouchstart="touchStart" bindtouchmove="touchMove" bindtouchend="touchEnd" bindtouchcancel="touchEnd">
</canvas>


<view style='height:300rpx;width:100%;background:#f2f2f2;'>
  <icon class='iconfont:icon-colour' style='color:red;'></icon>
  <text>{{str}}</text>
  <image src="data:image/svg+xml;base64," style='width:150rpx;height:150rpx' />
  <text>#123</text>
</view>

<!-- 遥控-->
<view style='' class='demo'>
<view class="button-group">
  <view class="outter-circle">
    <view class="inner-parts brown" bindtap="button" data-type="volAdd">
      <text class="nvfont:icon-up"></text>
    </view>
    <view class="inner-parts silver" bindtap="button" data-type="chaAdd">
      <text class="nvfont:icon-right"></text>
    </view>
    <view class="inner-parts blue" bindtap="button" data-type="chaDes">
      <text class="nvfont:icon-left"></text>
    </view>
    <view class="inner-parts gold" bindtap="button" data-type="volDes">
      <text class="nvfont:icon-down"></text>
    </view>

    <view class="inner-circle" bindtap="button" data-type="ok">
      <text class="enterfont:icon-enter" style='color : #F2F2F2'></text>
    </view>
  </view>
</view>
  
</view>


